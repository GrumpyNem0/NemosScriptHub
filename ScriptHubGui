-- Scripts:

local function ZGJDIY_fake_script() -- Center.TweeTwee 
	local script = Instance.new('LocalScript', Center)

	local ButtonOne = script.Parent["Nemo's Scripts"]
	local ButtonTwo = script.Parent["Script Hub"]
	local ButtonThree = script.Parent["Other"]
	---
	--{-1.068, 0},{-0.006, 0}
	ButtonOne.Position = (UDim2.new(-1.068, 0,-0.006, 0))
	ButtonTwo.Position = (UDim2.new(-1.068, 0,0.123, 0))
	ButtonThree.Position = (UDim2.new(-1.068, 0,0.253, 0))
	
	---
	
	wait(.6)
	ButtonOne:TweenPosition(UDim2.new(0.002, 0,-0.006, 0))
	wait(.3)
	ButtonTwo:TweenPosition(UDim2.new(0.002, 0,0.123, 0))
	wait(.3)
	ButtonThree:TweenPosition(UDim2.new(0.002, 0,0.253, 0))
	
	---
end
coroutine.wrap(ZGJDIY_fake_script)()
local function SUZLKA_fake_script() -- NemosScripts.ClickBttnScript 
	local script = Instance.new('LocalScript', NemosScripts)

	--SetText
	script.Parent.Text = script.Parent.Name
	--Locals
	local MainPageHolder = script.Parent.Parent.Parent.Parent.PageHolder
	local HomePage = MainPageHolder.Home
	local ScriptHPage = MainPageHolder.ScriptHub
	local OtherPage = MainPageHolder.Other
	--Function
	
	script.Parent.MouseButton1Click:Connect(function()
		HomePage.Visible = true
		ScriptHPage.Visible = false
		OtherPage.Visible = false
	end)
end
coroutine.wrap(SUZLKA_fake_script)()
