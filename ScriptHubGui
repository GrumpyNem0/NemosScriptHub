	_G.HomeScriptsFrame = script.Parent.Scripts
end
coroutine.wrap(KQGHUW_fake_script)()
local function OFBMLIA_fake_script() -- Main.Draggable 
	local script = Instance.new('LocalScript', Main)

	local UserInputService = game:GetService("UserInputService")local gui = script.Parent local dragging local dragInput local dragStart local startPos local function update(input)local delta = input.Position - dragStart gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)end gui.InputBegan:Connect(function(input)if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then dragging = true dragStart = input.Position startPos = gui.Position input.Changed:Connect(function()if input.UserInputState == Enum.UserInputState.End then dragging = false end end)end end)gui.InputChanged:Connect(function(input)if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then dragInput = input end end)UserInputService.InputChanged:Connect(function(input)if input == dragInput and dragging then update(input)end end)
end
coroutine.wrap(OFBMLIA_fake_script)()
local function HZUQBCY_fake_script() -- Title.TweeTwee 
	local script = Instance.new('LocalScript', Title)

	local TitleText = script.Parent.TextLabel
	local MinimizeButton = script.Parent.Minimize
	--
	TitleText.Position = (UDim2.new(-1, 0,0, 0))
	MinimizeButton.Position = (UDim2.new(2, 0,0, 0))
	--
	wait(.4)
	TitleText:TweenPosition(UDim2.new(0, 0,0, 0))
	wait(.2)
	MinimizeButton:TweenPosition(UDim2.new(0.94, 0, 0, 0))
	
end
coroutine.wrap(HZUQBCY_fake_script)()
local function XNMI_fake_script() -- Minimize.BttnScript 
	local script = Instance.new('LocalScript', Minimize)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.Parent.MainBackground.Size == UDim2.new(0, 500,0, 320) then
			script.Parent.Parent.Parent.MainBackground.PageHolder:TweenPosition(UDim2.new(3.682, 0,0.031, 0))
			script.Parent.Parent.Parent.MainBackground:TweenSize(UDim2.new(0, 0,0, 320))
		else
			script.Parent.Parent.Parent.MainBackground.PageHolder:TweenPosition(UDim2.new(0.282, 0,0.031, 0))
			script.Parent.Parent.Parent.MainBackground:TweenSize(UDim2.new(0, 500,0, 320))
		end
	end)
end
coroutine.wrap(XNMI_fake_script)()
